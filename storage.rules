rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Permite que usu√°rios autenticados leiam e escrevam em qualquer
    // arquivo dentro da pasta error_reports e suas subpastas.
    match /error_reports/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
