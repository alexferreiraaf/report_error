rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Permite o upload de arquivos para a pasta error_reports e suas subpastas
    // apenas para usu√°rios autenticados.
    match /error_reports/{folder}/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
